    "def toStr(n,base):\n",
    "    convertString='0123456789ABCDEF'\n",
    "    if n<base:\n",
    "        return convertString[n]\n",
    "    else:\n",
    "        return toStr(n//base,base)+convertString[n%base]"
   ]
